{
  "name": "frontend",
  "version": "0.3.1",
  "private": true,
  "main": "electron/dist/main.js",
  "homepage": "./",
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "electron:compile": "cd electron && tsc",
    "electron:compile:watch": "cd electron && tsc --watch",
    "electron": "pnpm electron:compile && NODE_ENV=development electron electron/dist/main.js",
    "electron:dev": "concurrently \"pnpm dev\" \"pnpm electron:compile:watch\" \"wait-on http://localhost:3000 && wait-on electron/dist/main.js && NODE_ENV=development electron electron/dist/main.js\"",
    "electron:build": "pnpm electron:compile && ELECTRON=true pnpm build && electron-builder",
    "electron:build:win": "pnpm electron:compile && ELECTRON=true pnpm build && electron-builder --win",
    "electron:build:mac": "pnpm electron:compile && ELECTRON=true pnpm build && electron-builder --mac",
    "electron:build:linux": "pnpm electron:compile && ELECTRON=true pnpm build && electron-builder --linux",
    "dev:electron": "pnpm electron:dev"
  },
